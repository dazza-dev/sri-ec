{
  "name": "dazza-dev/sri-ec",
  "description": "Paquete para generar, firmar y enviar documentos electrónicos (Factura, Guía de remisión, Nota crédito, Nota débito y Comprobante de retención) al SRI (Ecuador).",
  "keywords": [
    "sri",
    "signer",
    "firma",
    "firmar xml",
    "factura electrónica",
    "ecuador",
    "xades-bes"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "dazza-dev/sri-accesskey-generator": "^1.0",
    "dazza-dev/sri-sender": "^1.0",
    "dazza-dev/sri-signer": "^1.0",
    "dazza-dev/sri-xml-generator": "^1.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Andres Daza",
      "email": "adaza90@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DazzaDev\\SriEc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DazzaDev\\SriEc\\Tests\\": "tests/"
    }
  }
}
